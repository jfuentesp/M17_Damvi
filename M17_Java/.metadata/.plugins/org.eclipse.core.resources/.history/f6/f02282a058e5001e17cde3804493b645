package Classes;

public class Sprite extends Component {

	private GameObject m_Owner;
	private String m_Path;
	private String m_Description;
	
	public Sprite(GameObject owner, String path, String description) {
		super("Sprite");
		this.m_Owner = owner;
		this.m_Path = path;
		this.m_Description = description;
		System.out.println(String.format("Creado un componente Sprite para el objeto {0}, tomando el sprite <<{1}>> y con descripci√≥n {2}.", 
				m_Owner.getName(), path, description));
		
		
	}

	@Override
	public void update() {
		// TODO Auto-generated method stub
		
	}

}
